<div id="search">
    <button type="button" class="close">&times;</button>
    <form>
        <input type="search" value="" placeholder="Search your Git" />
        <button type="submit" class="btn btn-primary">Search</button>
    </form>
</div>
<div id="loadingbar-frame"></div>
<div class="frame" id="frame"></div>

<footer class="footer">
    <div class="container">
        <ul class="footer-links-list-copyright">
            <li> 
                <%- partial ('partials/gitlist-user-counter.ejs') %>  
           </li>
        </ul>
        <ul class="footer-links-list">
            <%- partial ('partials/gitlist-social-footer.ejs') %>  
            <hr>
        </ul>
    </div>
    <hr/>
    <div class="inner-gitlist-copyright">
        <%- partial ('partials/copyright.ejs') %>  
    </div>
</footer>
<script type="text/javascript">
    $("#clickme").loadingbar({
        done: function(data) {
            $.each( data.items, function( i, item ) {
                $( "<img/>" ).attr( "src", item.media.m ).prependTo( $("#frame") );
                if ( i === 2 ) {
                    return false;
                }
            });
        }
    });
</script>